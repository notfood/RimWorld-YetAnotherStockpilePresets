        <li>Ambrosia</li>
        <li>Beer</li>
      </disallowedThingDefs>
    </filter>
  </YetAnotherStockpilePresets.StockpilePresetDef>
  
  <YetAnotherStockpilePresets.StockpilePresetDef>
