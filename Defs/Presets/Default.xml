      <categories>
        <li>Medicine</li>
        <li>Drugs</li>
        <li>BodyParts</li>
      </categories>
      <thingDefs>
        <li>Neutroamine</li>
      </thingDefs>
      <disallowedThingDefs>
        <li>Ambrosia</li>
        <li>Beer</li>
      </disallowedThingDefs>
    </filter>
  </YetAnotherStockpilePresets.StockpilePresetDef>
  
  <YetAnotherStockpilePresets.StockpilePresetDef>
    <defName>Drugs</defName>
