    <filter>
      <categories>
        <li>FoodMeals</li>
      </categories>
      <specialFiltersToDisallow>
        <li>AllowRotten</li>
      </specialFiltersToDisallow>
    </filter>
  </YetAnotherStockpilePresets.StockpilePresetDef>
  
  <YetAnotherStockpilePresets.StockpilePresetDef>
    <defName>GeneralStorage</defName>
    <label>General Storage</label>
    <filter>
      <categories>
        <li>Manufactured</li>
        <li>ResourcesRaw</li>
        <li>Items</li>
        <li>Weapons</li>
        <li>Apparel</li>
        <li>Buildings</li>
        <li>CorpsesMechanoid</li>
        <li>Manufactured</li>
      </categories>
      <disallowedCategories>
        <li>PlantMatter</li>
      </disallowedCategories>
    </filter>
  </YetAnotherStockpilePresets.StockpilePresetDef>
  
  <YetAnotherStockpilePresets.StockpilePresetDef>
    <defName>GeneralMedical</defName>
