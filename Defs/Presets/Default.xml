<?xml version="1.0" encoding="utf-8" ?>

<Defs>

  <YetAnotherStockpilePresets.StockpilePresetDef>
    <defName>GeneralFreezer</defName>
    <label>General Freezer</label>
    <filter>
      <categories>
        <li>Foods</li>
        <li>PlantMatter</li>
        <li>CorpsesHumanlike</li>
        <li>CorpsesAnimal</li>
      </categories>
      <thingDefs>
        <li>Ambrosia</li>
        <li>Beer</li>
        <li>Wort</li>
      </thingDefs>
      <specialFiltersToDisallow>
        <li>AllowRotten</li>
      </specialFiltersToDisallow>
    </filter>
  </YetAnotherStockpilePresets.StockpilePresetDef>
  
  <YetAnotherStockpilePresets.StockpilePresetDef>
    <defName>GeneralFreezerNM</defName>
    <label>General Freezer (no meals)</label>
    <filter>
      <categories>
        <li>Foods</li>
        <li>PlantMatter</li>
        <li>CorpsesHumanlike</li>
        <li>CorpsesAnimal</li>
      </categories>
      <disallowedCategories>
        <li>FoodMeals</li>
      </disallowedCategories>
      <thingDefs>
        <li>Ambrosia</li>
        <li>Beer</li>
        <li>Wort</li>
      </thingDefs>
      <specialFiltersToDisallow>
        <li>AllowRotten</li>
      </specialFiltersToDisallow>
    </filter>
  </YetAnotherStockpilePresets.StockpilePresetDef>
  
  <YetAnotherStockpilePresets.StockpilePresetDef>
    <defName>Meals</defName>
